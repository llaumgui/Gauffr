{use $infos, $title}
<table summary="{$title}">
    <thead><tr>
    {foreach $infos as $info}
        {foreach $info as $key => $value}
        <th><label>{$key}</label></th>
        {/foreach}
        {break}
    {/foreach}
    </tr></thead>
    <tbody>
    {foreach $infos as $info}
    <tr>
        {foreach $info as $key => $value}
        <td>{$value}</td>
        {/foreach}
    </tr>
    {/foreach}
</tbody></table>